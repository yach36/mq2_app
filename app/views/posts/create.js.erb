
<!--モーダル非表示-->
var modalHide = () => {
    $postModal.fadeOut(fadeTime);
    $wrapper.animate({
      opacity: wrapperOpacity
    },
    fadeTime);
    $body.css({
      overflowY: "scroll"
    });
    $wrapper.css({
      pointerEvents: "auto",
    });
    $postBtn.animate({
      right: postBtnRight
    },
    fadeTime);
};
modalHide();
$(".l_main-wrapper").html("<%= j(render "posts/index", posts: @posts) %>");